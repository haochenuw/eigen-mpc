#include <obliv.oh>
#include "fixed.h"
#include "fixed.oh"
#include "test/test_fixed.h"
#include "util.h"
#include <stdio.h>


void test_fixed(void *v) {
	protocolIO *args = v;
	
	// check for equal length inputs
	bool equal;
	revealOblivBool(&equal, 
		feedOblivInt(args->len, 1) == 
		feedOblivInt(args->len, 2), 0);

	if(!equal) {
		args->len = -1;
		return;
	} 
	
	// compute the dot product of a and b
	ofixed_t c, x, y, t;
	ofixed_init(&c);
	ofixed_init(&x);
	ofixed_init(&y);
	ofixed_init(&t);
	double start_time = wallClock();

	
	for(size_t i = 0; i < args->len; i++) {
		if(i % 1000 == 0 && ocCurrentParty() == 2) {
			printf("%zd\t%f\n", i, wallClock() - start_time);
		}
		// assuming sizeof(int) == sizeof(fixed32_t)
		ofixed_import(&x, feedOblivLLong(args->inputs[i], 1));
		ofixed_import(&y, feedOblivLLong(args->inputs[i], 2));
		ofixed_mul(&t, x, y, args->p);
		ofixed_add(&c, c, t);
	}
	
	// oblivious givens rotation; 
	ofixed_import(&c, 0);
	ofixed_eig2(&c,x,y,t,args->p); 

	// ofixed_sqrt(&c, c, args->p);
	// ofixed_givens(&c, &t, x, y, args->p); 
    //obliv fixed_t oute = ofixed_export(e);
	//obliv fixed_t outf = ofixed_export(f);
	//revealOblivLLong(&(args->result), oute, 0); 
	//revealOblivLLong(&(args->result + 1), outf, 0); 



	obliv fixed_t out = ofixed_export(c);
	revealOblivLLong(&(args->result), out, 0);
	args->gates = yaoGateCount();

	ofixed_free(&c);
	ofixed_free(&x);
	ofixed_free(&y);
	ofixed_free(&t);
}
